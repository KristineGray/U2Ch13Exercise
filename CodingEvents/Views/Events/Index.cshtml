@* 4. Replace the ul with a table to display event names and descriptions.*@

<h1>Coding Events</h1>

<p>
    <a asp-controller="Events" asp-action="Add">Add Event</a>
</p>

@if (ViewBag.events.Count == 0)
{
    <p>No events yet!</p>
}

<ul>
    @foreach (string name in ViewBag.events)
    {
        <li>@name</li>
    }
</ul>

